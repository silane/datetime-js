{
  "name": "@silane/datetime",
  "version": "0.1.0",
  "description": "Date and time library similar to Python's \"datetime\" package.",
  "type": "module",
  "module": "./src/index.js",
  "main": "./lib/index.cjs",
  "scripts": {
    "test": "jest",
    "build": "npm run build:cjs && npm run build:browser",
    "build:cjs": "babel src --env-name node-cjs --out-dir lib --out-file-extension .cjs",
    "build:browser": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silane/datetime-js.git"
  },
  "keywords": [
    "datetime",
    "date",
    "time",
    "timedelta",
    "duration",
    "timezone"
  ],
  "author": "silane",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "datetime.test.js",
    "jest.config.cjs",
    "babel.config.cjs",
    "rollup.config.js",
    "src/*",
    "lib/*",
    "umd/*"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-jest": "^26.0.1",
    "babel-plugin-replace-import-extension": "^1.0.1",
    "jest": "^26.0.1",
    "rollup": "^2.8.2",
    "rollup-plugin-babel": "^4.4.0"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
